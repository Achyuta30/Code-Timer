{
  "name": "code-timer-extension",
  "displayName": "Code Execution Timer",
  "description": "Measure code execution time for Java, C++, Python, JavaScript with one click",
  "version": "1.0.0",
  "publisher": "YourName",
  "license": "MIT",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Debuggers",
    "Testing"
  ],
  "main": "./dist/shared/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "code-timer.run",
        "title": "Run and Time Code"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "package": "vscode-vsce package",
    "test": "jest"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^20.14.2",
    "@types/vscode": "^1.101.0",
    "@vscode/vsce": "^2.26.0",
    "jest": "^30.0.4",
    "ts-jest": "^29.4.0",
    "typescript": "^5.5.2"
  }
}